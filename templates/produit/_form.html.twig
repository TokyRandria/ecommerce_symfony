<div class="container">
{{ form_start(form) }}
    <div class="my-custom-class-for-errors">
        {{ form_errors(form) }}
    </div>
    <div class="row">
        <div class="col">
            {{ form_row(form.libelle) }}
        </div>
        <div class="col" id="some-custom-id">
            {{ form_row(form.famille) }}
        </div>
    </div>
    {{ form_widget(form) }}
<button class="btn btn-info">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}

</div>
